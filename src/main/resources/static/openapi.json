{
  "openapi": "3.0.1",
  "info": {
    "title": "Store API",
    "description": "Store API Documentation",
    "contact": {},
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://127.0.0.1:8080/"
    }
  ],
  "tags": [
    {
      "name": "store",
      "description": "Stores"
    }
  ],
  "paths": {
    "/store": {
      "get": {
        "tags": [
          "store"
        ],
        "summary": "Get nearest stores based on latitude/longitude",
        "operationId": "getNearestStoresUsingGET",
        "parameters": [
          {
            "name": "latitude",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "longitude",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Page.Store"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/store/{uuid}": {
      "get": {
        "tags": [
          "store"
        ],
        "summary": "Get store by uuid",
        "operationId": "getStoreUsingGET",
        "parameters": [
          {
            "name": "uuid",
            "in": "path",
            "description": "uuid",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Store"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {}
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Pageable": {
        "title": "Pageable",
        "type": "object",
        "properties": {
          "offset": {
            "type": "integer",
            "format": "int64"
          },
          "pageNumber": {
            "type": "integer",
            "format": "int32"
          },
          "pageSize": {
            "type": "integer",
            "format": "int32"
          },
          "paged": {
            "type": "boolean"
          },
          "sort": {
            "$ref": "#/components/schemas/Sort"
          },
          "unpaged": {
            "type": "boolean"
          }
        }
      },
      "ErrorResponse": {
        "title": "Error",
        "type": "object",
        "properties": {
          "dateTime": {
            "type": "string",
            "format": "date-time"
          },
          "message": {
            "type": "string",
            "example": "Invalid coordinates."
          }
        }
      },
      "Page.Store": {
        "title": "Page«Store»",
        "type": "object",
        "properties": {
          "content": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Store"
            }
          },
          "empty": {
            "type": "boolean"
          },
          "first": {
            "type": "boolean"
          },
          "last": {
            "type": "boolean"
          },
          "number": {
            "type": "integer",
            "format": "int32"
          },
          "numberOfElements": {
            "type": "integer",
            "format": "int32"
          },
          "pageable": {
            "$ref": "#/components/schemas/Pageable"
          },
          "size": {
            "type": "integer",
            "format": "int32"
          },
          "sort": {
            "$ref": "#/components/schemas/Sort"
          },
          "totalElements": {
            "type": "integer",
            "format": "int64"
          },
          "totalPages": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "Sort": {
        "title": "Sort",
        "type": "object",
        "properties": {
          "empty": {
            "type": "boolean"
          },
          "sorted": {
            "type": "boolean"
          },
          "unsorted": {
            "type": "boolean"
          }
        }
      },
      "Store": {
        "title": "Store",
        "type": "object",
        "properties": {
          "addressName": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "collectionPoint": {
            "type": "boolean"
          },
          "complexNumber": {
            "type": "string"
          },
          "latitude": {
            "type": "number",
            "format": "double"
          },
          "locationType": {
            "type": "string"
          },
          "longitude": {
            "type": "number",
            "format": "double"
          },
          "postalCode": {
            "type": "string"
          },
          "showWarningMessage": {
            "type": "boolean"
          },
          "street": {
            "type": "string"
          },
          "street2": {
            "type": "string"
          },
          "street3": {
            "type": "string"
          },
          "todayClose": {
            "type": "string"
          },
          "todayOpen": {
            "type": "string"
          },
          "uuid": {
            "type": "string"
          }
        }
      }
    }
  }
}